    <cache name="ru.otus.dobrovolsky.dataSet.UserDataSet"
           maxEntriesLocalHeap="100"
           eternal="false"
           timeToLiveSeconds="90">
    </cache>
    <cache name="ru.otus.dobrovolsky.dataSet.PhoneDataSet"
           maxEntriesLocalHeap="100"
           eternal="false"
           timeToLiveSeconds="90">
    </cache>
    <cache name="ru.otus.dobrovolsky.dataSet.AddressDataSet"
           maxEntriesLocalHeap="100"
           eternal="false"
           timeToLiveSeconds="90">
    </cache>
    <cache name="userById"
           maxEntriesLocalHeap="100"
           eternal="false"
           timeToIdleSeconds="90">
    </cache>
    <cache name="namedQuery"
           maxEntriesLocalHeap="100"
           eternal="false"
           timeToIdleSeconds="90">
