<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ehcache>
    <defaultCache maxEntriesLocalHeap="10" eternal="false" timeToIdleSeconds="600" timeToLiveSeconds="600">
    </defaultCache>

    <cache name="ru.otus.dobrovolsky.base.dataSets.UserDataSet"
           maxEntriesLocalHeap="100"
           eternal="false"
           timeToLiveSeconds="600">
    </cache>
    <cache name="ru.otus.dobrovolsky.base.dataSets.PhoneDataSet"
           maxEntriesLocalHeap="100"
           eternal="false"
           timeToLiveSeconds="600">
    </cache>
    <cache name="ru.otus.dobrovolsky.base.dataSets.AddressDataSet"
           maxEntriesLocalHeap="100"
           eternal="false"
           timeToLiveSeconds="600">
    </cache>
    <cache name="userById"
           maxEntriesLocalHeap="100"
           eternal="false"
           timeToIdleSeconds="600">
    </cache>
    <cache name="namedQuery"
           maxEntriesLocalHeap="100"
           eternal="false"
           timeToIdleSeconds="600">
    </cache>
    <cache name="org.hibernate.cache.internal.StandardQueryCache"
           maxElementsInMemory="10000"
           eternal="false"
           timeToLiveSeconds="600"
           overflowToDisk="false"
           memoryStoreEvictionPolicy="LRU">
    </cache>
</ehcache>